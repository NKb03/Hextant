{
  "definitions": {},
  "$schema": "",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Plugin Schema",
  "default": null,
  "required": [
    "plugin"
  ],
  "properties": {
    "plugin": {
      "$id": "#/properties/plugin",
      "type": "object",
      "title": "The Plugin Info Schema",
      "default": null,
      "required": [
        "name",
        "author"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/plugin/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "Hextant"
          ],
          "pattern": ".+"
        },
        "author": {
          "$id": "#/properties/plugin/properties/author",
          "type": "string",
          "title": "The Author Schema",
          "default": "",
          "examples": [
            "Nikolaus Knop"
          ],
          "pattern": ".+"
        }
      }
    },
    "editables": {
      "$id": "#/properties/editables",
      "type": "array",
      "title": "The Editables Schema",
      "default": null,
      "items": {
        "$id": "#/properties/editables/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "class",
          "edited"
        ],
        "properties": {
          "edited": {
            "$id": "#/properties/editables/items/properties/edited",
            "type": "string",
            "title": "The Edited Schema",
            "default": "",
            "examples": [
              "org.nikok.core.expr.edited.IntLiteral"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          },
          "editable": {
            "$id": "#/properties/editables/items/properties/edited",
            "type": "string",
            "title": "The Editable Schema",
            "default": "",
            "examples": [
              "org.nikok.core.expr.editable.EditableIntLiteral"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          }
        }
      }
    },
    "editors": {
      "$id": "#/properties/editors",
      "type": "array",
      "title": "The Editors Schema",
      "default": null,
      "items": {
        "$id": "#/properties/editors/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "editable",
          "editor"
        ],
        "properties": {
          "editable": {
            "$id": "#/properties/editors/items/properties/editable",
            "type": "string",
            "title": "The Editable Schema",
            "default": "",
            "examples": [
              "org.nikok.hextant.command.line.CommandLine"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          },
          "editor": {
            "$id": "#/properties/editors/items/properties/editor",
            "type": "string",
            "title": "The Editor Schema",
            "default": "",
            "examples": [
              "org.nikok.hextant.command.line.CommandLineController"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          }
        }
      }
    },
    "views": {
      "$id": "#/properties/views",
      "type": "array",
      "title": "The Views Schema",
      "default": null,
      "items": {
        "$id": "#/properties/views/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "editable",
          "view"
        ],
        "properties": {
          "editable": {
            "$id": "#/properties/views/items/properties/editable",
            "type": "string",
            "title": "The Editable Schema",
            "default": "",
            "examples": [
              "org.nikok.hextant.command.line.CommandLine"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          },
          "view": {
            "$id": "#/properties/views/items/properties/view",
            "type": "string",
            "title": "The View Schema",
            "default": "",
            "examples": [
              "org.nikok.hextant.command.line.FXCommandLineView"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          }
        }
      }
    },
    "inspections": {
      "$id": "#/properties/inspections",
      "type": "array",
      "title": "The Inspections Schema",
      "default": null,
      "items": {
        "$id": "#/properties/inspections/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "class"
        ],
        "properties": {
          "class": {
            "$id": "#/properties/inspections/items/properties/class",
            "type": "string",
            "title": "The Class Schema",
            "default": "",
            "examples": [
              "org.nikok.hextant.AnInspection"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          }
        }
      }
    },
    "commands": {
      "$id": "#/properties/commands",
      "type": "array",
      "title": "The Commands Schema",
      "default": null,
      "items": {
        "$id": "#/properties/commands/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "class"
        ],
        "properties": {
          "class": {
            "$id": "#/properties/commands/items/properties/class",
            "type": "string",
            "title": "The Class Schema",
            "default": "",
            "examples": [
              "org.nikok.hextant.ACommand"
            ],
            "pattern": "(([A-Za-z0-9_]+\\.?)*[A-Za-z0-9_]+)$"
          }
        }
      }
    }
  }
}